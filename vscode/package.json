{
  "name": "mo-links",
  "displayName": "Mo Links",
  "description": "Treats mo/some-thing as a clickable link, to be used with the Chrome extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "publisher": "MoLinks",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:ruby",
    "onLanguage:php",
    "onLanguage:python",
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:css",
    "onLanguage:html",
    "onLanguage:javascriptreact",
    "onLanguage:markdown",
    "onLanguage:plaintext",
    "onLanguage:rust",
    "onLanguage:shellscript",
    "onLanguage:sql",
    "onLanguage:svelte",
    "onLangugage:swift",
    "onLanguage:typescriptreact",
    "onLanguage:vue",
    "onLanguage:vue-html",
    "onLanguage:yaml"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "mo-links.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
